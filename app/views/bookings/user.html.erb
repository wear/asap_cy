<div id="booking_form">

<div id="middle">
	<div id="flash_error">
		
	</div>
	<div class='span-4'>
 		<h3>注册用户预定</h3>
		<% form_tag  new_vendor_booking_path(@vendor), :method => :get do -%>
			<%= render :partial => '/sessions/form' %> 
			<p><%= submit_tag 'Log in' %></p>          
		<% end -%>   
	</div> 

	<div class='span-5'>
	  	<h3>手机预定</h3>
	<% form_tag new_vendor_booking_path(@vendor), :method => :get do -%>
		<p><%= label_tag 'mobile' %><br/>  
		<%= text_field_tag :mobile %></p>
		<p><%= submit_tag 'Log in' %></p>  
	<% end -%>
	</div>
</div>

	<div id="bottom">
	</div>
</div>