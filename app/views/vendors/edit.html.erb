
<% form_for @vendor do |f| %> 
	<p>
	<%= label_tag "名称"%><br />
	<%= f.text_field :name %></p>
   <p>
	<%= label_tag "别名"%><br />       
	<%= f.text_field :alias %></p>
	<p>
	<%= label_tag "地址"%><br />       
	<%= f.text_field :address %></p>
	<p>                                                           
	<%= label_tag "折扣"%><br />     
	<%= f.select :discount_id, @discounts.map{|d| [d.name,d.id]}.insert(0,['请选择',nil])%></p>
	<p>提示:补充完整的网站信息有助您的排位</p>
	<%= submit_tag "提交" %>
<% end %> 
<%= link_to '返回',user_discounts_path(current_user) %>