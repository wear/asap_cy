<div id="vendor">
	<div id="vendor-title">
		 <%=link_to h(vendor.name),vendor_path(vendor),:class => "title"%>
	      <ul class="rating <%=star(vendor.total)%>">
	      <li class="one"><a href="#" title="1 Star">1</a></li>
	      <li class="two"><a href="#" title="2 Stars">2</a></li>
	      <li class="three"><a href="#" title="3 Stars">3</a></li>
	      <li class="four"><a href="#" title="4 Stars">4</a></li>
	      <li class="five"><a href="#" title="5 Stars">5</a></li>
	      </ul>
	</div>
	<div id="vendor-get">
		<%=image_tag "bg/yuding.gif"%>
	</div>
	<div id="vendor-count">
		预定人数
	</div>

	<div id="vendor-body">
		<div id="vendor-info">
		      地址:<%=h vendor.address%>(靠近<%=h vendor.nearby%>) <br />
		   电话:<%=h vendor.tel1%> <%=h vendor.tel2%> 
		</div>
		<div id='vendor-rate'>
			<% if vendor.discount.nil? -%>
			暂无
			<% else %>
			<span class="discount"><%= vendor.discount.count %></span>
			<% end %>
		</div>   
	</div>

</div>     