<tr class='first_row_class'>
	    <td >&nbsp;</td>
	    <td class='vendor-name'><span class='vendor_title'><%= h(vendor.name)%></span></td> 
	    <td><%= vendor.taste/10 %></td> 
	    <td><%= vendor.service/10 %></td> 
	    <td><%= vendor.env/10 %></td>	
		<td><span class='rmb'>￥</span><%= vendor.avg.to_i %></td>
		<td><span class='B'><%= format_discount(vendor) %></span></td>
	    <td><span class='B'><%= (((vendor.sum*10).round)/10.0) %></span></td>
		<td class="collapsible"></td>
</tr> 
<tr class="expand-child"> 
  <td>&nbsp;</td>
  <td colspan="5">
  	<div class="bold"><%= vendor.address %><%=link_to '地图', map_vendor_path(vendor),:target => '_blank' %>&nbsp;<%=fengdian_link(vendor)%></div>
  	<div>
        电话: <%= vendor.tel1%><br/>
          标签: <%= vendor.tag_list %></td>
  	</div>
  </td>
  <td colspan='3'>星级:<%= render :partial => 'shared/star',:locals => { :vendor => vendor }%>     </td>
</tr> 
<tr class="expand-child" id='info-bottom'> 
  <td class='left_border'>&nbsp;</td>
	<td colspan="8" class='right_border'>
		<ul id='opration'>
			<li>收藏</li>
			<li><%=booking_vendor_path(vendor)%><li>
			<li>举报</li>
		</ul>
	</td>
</tr>
  